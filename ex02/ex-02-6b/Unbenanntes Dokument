int UC_PIN = 12;

int val = 0;
long millies;
boolean setupUC = false;

void setup() {
  Serial.begin(9600);
}

void loop() {
  if(val == 0) {
    pinMode(UC_PIN, OUTPUT);  
    digitalWrite(UC_PIN, HIGH);
    delayMicroseconds(11);
    digitalWrite(UC_PIN, LOW);  
    pinMode(UC_PIN, INPUT);
    setupUC = true;
    Serial.println("Setup complete");
  }
  val = digitalRead(UC_PIN);
  Serial.println(val);
}

